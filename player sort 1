package evernorth.demos.model;

public class Player implements Comparable<Player>{

	private int playerId;
	private String name;
	private String team;

	public Player() {
		// TODO Auto-generated constructor stub
	}

	public Player(int playerId, String name, String team) {
		super();
		this.playerId = playerId;
		this.name = name;
		this.team = team;
	}

	public int getPlayerId() {
		return playerId;
	}

	public void setPlayerId(int playerId) {
		this.playerId = playerId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getTeam() {
		return team;
	}

	public void setTeam(String team) {
		this.team = team;
	}

	@Override
	public String toString() {
		return "Player [playerId=" + playerId + ", name=" + name + ", team=" + team + "]";
	}

	@Override
	public int compareTo(Player o) {
		//playerid
		
		return this.playerId - o.playerId; // this first asc
		//return o.playerId - this.playerId; // object fst desc
	}
	
	
	
	
}



---------------------------service 




package evernorth.demos.service;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

import evernorth.demos.model.Player;

public class PlayerService {

	public static void main(String[] args) {

		
		List<Player> players=new ArrayList<Player>();
		
	Player player1=new Player(45, "Rohit", "MI");	
	Player player2=new Player(18, "Virat", "RCB");
	Player player3=new Player(1, "Rahul", "DC");
	Player player4=new Player(7, "MSD", "CSK");
	Player player5=new Player(99, "Ashwin", "CSK");
	
		
	players.add(player1);
	players.add(player2);
	players.add(player3);
	players.add(player4);
	players.add(player5);
	
	
	Collections.sort(players);
	
	for(Player player:players)
	System.out.println(player);
	
	}

}

