package evernorth.demos.service;

import java.util.ArrayList;
import java.util.List;

import evernorth.demos.model.Player;

public class PlayerService {

	public static void main(String[] args) {
	
		Player player1=new Player(18, "virat", "rcb");
		Player player2=new Player(45, "rohit", "mi");
		Player player3=new Player(1, "rahul", "dc");
		
		
		List<Player> players=new ArrayList<Player>();
		
		players.add(player3);
		players.add(player2);
		players.add(player1);
	
		players.forEach(
				x->System.out.println(x)
				);
	
	}

}
