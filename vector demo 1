//list -> allows duplicates- > maintains order

//vector -> thread safe -> slow -> should be used in multi threaded env


import java.util.ArrayList;
import java.util.Iterator;
import java.util.List;
import java.util.Vector;


public class MyDemos {

	
	void listDemo(List<Integer> names)
	{
		int x=10;
		names.add(x);
		//names.add("hello");
		//names.add("virat");
		//names.add("rohit");
		//names.add("virat");

		Iterator<String> itr= names.iterator();

		 while(itr.hasNext()) 
		 {
			 String element=itr.next();
			 if(element.equals("hello")) {
				 itr.remove();
			 }
			//System.out.println(element);
			
		 }
		 System.out.println(names);
	}
	
	
	public static void main(String[] args) {

			
		new MyDemos().listDemo(new ArrayList<String>());
	
	}

}
