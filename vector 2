//list -> allows duplicates- > maintains order

//vector -> thread safe -> slow -> should be used in multi threaded env


import java.util.ArrayList;
import java.util.List;
import java.util.Vector;


public class MyDemos {

	
	void listDemo(List<String> names)
	{
		names.add("hello");
		names.add("virat");
		names.add("rohit");
		names.add("virat");
			
		//names.add(1, "new data");
		boolean isPresent= names.contains("virat");
		System.out.println(isPresent);
		System.out.println(names);

	}
	
	
	public static void main(String[] args) {

			
		new MyDemos().listDemo(new ArrayList<String>());
	
	}

}
